#!/usr/bin/env python3

import requests
import json

city = "mazeikiai"
url = f"https://api.meteo.lt/v1/places/{city}/forecasts/long-term"
response = requests.get(url)
#status_code = response.status_code
data = response.json()
text = json.dumps(data, sort_keys=True, indent=4)
with open('weather_report.json', 'w') as file:
    file.write(text)
